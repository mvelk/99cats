
<h1>Cat Rental Requests</h1>

<ul>
  <% @requests.each do |request| %>
    <li>
      <a href=<%= cat_rental_request_url(request) %>> <%= "#{request.cat_id} from #{request.start_date} to #{request.end_date}" %></a>
    </li>
    <%= button_to 'delete', cat_rental_request_url(request), method: :delete %>
  <% end %>
</ul>
